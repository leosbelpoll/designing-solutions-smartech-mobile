{
    "expo": {
        "name": "Smartech App",
        "slug": "smartech-app",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icono-app.png",
        "scheme": "myapp",
        "splash": {
            "image": "./assets/images/logo-app1.png",
            "resizeMode": "contain",
            "backgroundColor": "#FFFAFA"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "web": {
            "favicon": "./assets/images/icono-app.png"
        },
        "sdkVersion": "37.0.0",
        "android": {
            "package": "com.designingsolutions.smartechapp",
            "versionCode": 1,
            "permissions": ["CAMERA", "CAMERA_ROLL", "NOTIFICATIONS"],
            "googleServicesFile": "./google-services.json"
        },
        "ios": {
            "bundleIdentifier": "com.designingsolutions.smartechapp"
        },
        "notification": {
            "icon": "./assets/images/icono-app.png"
        }
    }
}
